{
  "manifest_version": 2,
  "name": "Maths for Glean",
  "description": "Add maths formatting to Glean notes",
  "version": "2024.1.1",
  "icons": {
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://app.glean.co/event/*"
      ],
      "js": [
        "content_script.ts"
      ],
      "css" : [
        "maths.css",
        "glean-font-override.css"
      ]
    }
  ]
}
